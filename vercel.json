{
    "version": 2,
    "name": "by-ndeyaa",
    "builds": [
        {
            "src": "mailer.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mailer.js"
        }
    ],
    "crons": [
    {
      "path": "/api/send-test-mail-cron-job",
      "schedule": "*/10 * * * *"
    }
  ]

}